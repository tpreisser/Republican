        <div class="navigation">
            <button class="nav-button prev" onclick="previousSlide()">
                <span>←</span>
                <span>PREVIOUS</span>
            </button>
            <div class="slide-counter">
                <span id="current-slide">1</span> / <span id="total-slides">13</span>
            </div>
            <button class="nav-button next" onclick="nextSlide()">
                <span>NEXT</span>
                <span>→</span>
            </button>
